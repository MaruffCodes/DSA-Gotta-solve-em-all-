// Sorting using Insertion Sort
#include <bits/stdc++.h>
using namespace std;
int main() {
    int n=6;
    int arr[n]={13,46,24,52,20,9};
    cout <<"Arr :";
    for (int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }
    //Insertion Sort
    for (int i=0;i<n;i++){
        int j=i;
        while (j>0 && arr[j-1]>arr[j]){
            int temp=arr[j-1];
            arr[j-1]=arr[j];
            arr[j]=temp;
            
            j--;
        }
    }
    cout <<endl<<"Sorted arr :";
    for (int i=0;i<n;i++)
        cout <<arr[i]<<" ";

    return 0;
}
