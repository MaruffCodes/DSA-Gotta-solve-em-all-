// Find number of repetition of using hashing
#include <iostream>
using namespace std;
int main() {

    int n;
    cout <<"Enter n";
    cin >>n;
    int arr[n];
    for (int i=0;i<n;i++){
        cin >>arr[i];
    }
    //preStoring
    int hashh[13]={0};
    for (int i=0;i<n;i++){
        hashh[arr[i]]+=1;
    }
    
    int p;
    cin >>p;
    while (p--){
        int num;
        cin >>num;
        
        //fetching
       cout << hashh[num];
        
    }
    

    return 0;
}
