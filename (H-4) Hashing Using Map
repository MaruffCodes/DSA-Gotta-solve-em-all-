//Hashing using Map in cpp
#include <bits/stdc++.h>
using namespace std;
int main() {
    int n;
    cin >>n;
    int arr[n];
    for (int i=0;i<n;i++){
        cin >>arr[i];
    }
    //pre-computing
    map<int,int> mpp;
    for (int i=0;i<n;i++)
        mpp[arr[i]]++;
    //Displaying Key Value Pair
    for (auto i :mpp)
        cout <<i.first <<"->"<<i.second;
    int q;
    cin>>q;
    while (q--){
        int num;
        cin>>num;
        cout<<mpp[num]<<" ";
    }
    return 0;
}
